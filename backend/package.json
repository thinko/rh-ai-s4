{
  "name": "s4-backend",
  "version": "0.0.0-see-root",
  "description": "Backend for S4 - Super Simple Storage Service",
  "author": "Red Hat AI Services BU",
  "license": "Apache-2.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rh-aiservices-bu/s4.git"
  },
  "homepage": "https://github.com/rh-aiservices-bu/s4",
  "bugs": {
    "url": "https://github.com/rh-aiservices-bu/s4/issues"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "main": "src/server.ts",
  "scripts": {
    "clean": "rm -f ../logs/adminActivity.log",
    "start": "NODE_ENV=production PORT=8888 node ./dist/server.js --log=1 --registry=localhost:50051",
    "start:dev": "npm run clean && export NODE_TLS_REJECT_UNAUTHORIZED=0 && export NODE_ENV=development && export PORT=8888 && nodemon src/server.ts --log=1 --registry=localhost:50051",
    "start:dev:log": "npm run clean && export NODE_TLS_REJECT_UNAUTHORIZED=0 && export NODE_ENV=development && export PORT=8888 && nodemon src/server.ts --log=1 --registry=localhost:50051 2>&1 | tee backend-debug-$(date +%Y%m%d-%H%M%S).log",
    "debug": "npm run tsc && export NODE_TLS_REJECT_UNAUTHORIZED=0 && export NODE_ENV=development && node --inspect ./dist/server.js --log=1 --registry=localhost:50051",
    "build-only": "tsc -p . && node ./dist/server.js --log=1 --registry=localhost:50051 --buildonly",
    "build": "run-s build:clean tsc:prod",
    "build:clean": "rimraf ./dist",
    "test": "run-s test:lint test:type-check test:jest",
    "test:lint": "eslint --max-warnings 0 --ext .json,.js,.ts src/plugins src/routes src/utils",
    "test:fix": "eslint --ext .json,.js,.ts src/plugins src/routes src/utils --fix",
    "test:type-check": "tsc --noEmit",
    "test:jest": "jest --coverage",
    "server": "NODE_ENV=production node ./dist/server.js",
    "tsc": "tsc -p .",
    "tsc:prod": "tsc -p tsconfig.prod.json",
    "lint": "eslint ./src/",
    "lint:fix": "eslint ./src/ --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "watch": "tsc -p . -w"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.787.0",
    "@aws-sdk/lib-storage": "^3.787.0",
    "@aws-sdk/middleware-apply-body-checksum": "^3.370.0",
    "@aws-sdk/node-http-handler": "^3.374.0",
    "@fastify/accepts": "^5.0.2",
    "@fastify/autoload": "^6.3.1",
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^11.2.0",
    "@fastify/helmet": "^13.0.1",
    "@fastify/multipart": "^9.0.3",
    "@fastify/sensible": "^6.0.3",
    "@fastify/static": "^9.0.0",
    "@fastify/swagger": "^9.7.0",
    "@fastify/websocket": "^11.2.0",
    "@smithy/types": "^4.2.0",
    "@types/jsonwebtoken": "^9.0.10",
    "axios": "^1.8.4",
    "dotenv": "^16.5.0",
    "fastify": "^5.7.2",
    "fastify-plugin": "^5.1.0",
    "fastify-sse-v2": "^4.2.1",
    "http-errors": "^1.8.0",
    "http-proxy-agent": "^7.0.2",
    "https-proxy-agent": "^7.0.6",
    "js-yaml": "^4.0.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "minipass": "^7.1.2",
    "p-limit": "^3.1.0",
    "pino": "^8.11.0",
    "prom-client": "^14.0.1",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@types/form-data": "^2.2.1",
    "@types/http-errors": "^1.8.0",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.14.182",
    "@types/node": "^20.19.33",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "aws-sdk-client-mock": "^4.1.0",
    "babel-eslint": "^10.1.0",
    "fastify-cli": "^7.4.0",
    "form-data": "^4.0.5",
    "memfs": "^4.6.0",
    "nodemon": "^3.0.2",
    "pino-pretty": "^9.4.0",
    "prettier": "3.5.3",
    "rimraf": "^5.0.7",
    "ts-protoc-gen": "^0.10.0",
    "tslib": "^2.0.3",
    "typescript": "^5.3.3"
  },
  "optionalDependencies": {
    "@types/jest": "^29.5.3",
    "eslint": "^8.57.0",
    "eslint-config-esnext": "^4.1.0",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.6.1",
    "ts-jest": "^29.1.1"
  },
  "overrides": {
    "tough-cookie": "^4.1.3",
    "ws": "^8.17.1",
    "@types/tar": "^6.1.13",
    "jsonpath-plus": "^10.3.0"
  }
}
